{
    "name": "crzhi/laravel-zipper",
    "description": "This is a simple Wrapper around the ZipArchive methods with some handy functions.",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "crzhi",
            "email": "crzhi9115@163.com"
        }
    ],
    "require": {
        "php": ">=7.2.0",
        "ext-zip": "*",
        "illuminate/support": "^5.8|^6.0|^7.0",
        "illuminate/filesystem": "^5.8|^6.0|^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0|^8.8",
        "mockery/mockery": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "Chumper\\Zipper\\": "src/Chumper/Zipper"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Chumper\\Zipper\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Chumper\\Zipper\\ZipperServiceProvider"
            ],
            "aliases": {
                "Zipper": "Chumper\\Zipper\\Zipper"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}